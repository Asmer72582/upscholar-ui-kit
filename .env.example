# UpScholar Frontend Environment Variables
# Copy this file to .env and update the values

# Backend API URL
VITE_API_BASE_URL=https://api.upscholar.in

# Socket.io URL (usually same as API URL)
VITE_SOCKET_URL=https://api.upscholar.in

# Frontend URL
VITE_FRONTEND_URL=http://localhost:8080

# Environment Mode (development, production, staging)
VITE_MODE=development
